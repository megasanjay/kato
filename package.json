{
  "name": "kato",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "build": "prisma generate && nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "lint:js": "eslint --ext \".ts,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev --preview-feature",
    "prisma:studio": "prisma studio",
    "prisma:db:push": "prisma db push",
    "prisma:db:pull": "prisma db pull --preview-feature",
    "scripts:generate:background:images": "ts-node --project tsconfig.tsnode.json --esm --swc ./scripts/generate-background-images.mts",
    "scripts:generate:affirmations": "ts-node --project tsconfig.tsnode.json --esm --swc ./scripts/generate-affirmations.mts",
    "postinstall": "nuxt prepare"
  },
  "devDependencies": {
    "@bg-dev/nuxt-naiveui": "^1.1.1",
    "@faker-js/faker": "^8.0.2",
    "@nuxt/devtools": "^0.5.5",
    "@nuxt/image": "^1.0.0-rc.1",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/html-validator": "^1.4.0",
    "@nuxtjs/tailwindcss": "^6.7.0",
    "@pinia-plugin-persistedstate/nuxt": "^1.1.1",
    "@swc/core": "^1.3.66",
    "@types/node": "^18",
    "@types/validator": "^13.7.17",
    "@typescript-eslint/parser": "^5.59.6",
    "axios": "^1.4.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sort-keys-custom-order": "^1.0.5",
    "eslint-plugin-unused-imports": "^2.0.0",
    "nuxt": "^3.5.0",
    "nuxt-icon": "^0.4.1",
    "nuxt-lodash": "^2.4.1",
    "nuxt-seo-kit": "^1.3.8",
    "plaiceholder": "^3.0.0",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "prisma": "^4.14.1",
    "sharp": "^0.32.1",
    "tailwindcss-debug-screens": "^2.2.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.4.11",
    "@prisma/client": "^4.14.1",
    "@sentry/tracing": "^7.57.0",
    "@sentry/vue": "^7.57.0",
    "dayjs": "^1.11.7",
    "nanoid": "^4.0.2",
    "pinia": "^2.1.3",
    "validator": "^13.9.0",
    "vue3-lottie": "^2.7.0",
    "zod": "^3.21.4"
  }
}
