// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Background {
  id String @id @default(cuid())

  theme String
  url   String

  date String

  created DateTime @default(now())
}
